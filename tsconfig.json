{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "rootDirs": ["src", "types"],
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  }
}
